<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="1000000" id="com.sound.level.swiftlet" version="1.0.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="emiAdmobPlugin">
        <param name="android-package" value="emi.indo.cordova.plugin.admob.emiAdmobPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SoundMeter">
        <param name="android-package" value="sound.level.meter.SoundMeter" />
    </feature>
    <name>Sound Level Swiftlet</name>
    <description>Sound Level master Swiftlet</description>
    <author email="meylan.sb@gmail.com" href="">SAMBAS DEV</author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-navigation href="https://*/*" />
    <allow-navigation href="blob:*" />
    <icon density="xxxhdpi" height="512" src="www/icons/icon-512.png" width="512" />
    <icon height="256" src="www/icons/icon-256.png" width="256" />
    <icon density="xhdpi" height="128" src="www/icons/icon-128.png" width="128" />
    <icon density="mdpi" height="64" src="www/icons/icon-64.png" width="64" />
    <icon height="32" src="www/icons/icon-32.png" width="32" />
    <icon height="16" src="www/icons/icon-16.png" width="16" />
    <access origin="*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="AndroidXEnabled" value="true" />
    <preference name="GradlePluginKotlinEnabled" value="true" />
    <preference name="GradlePluginKotlinVersion" value="2.1.0" />
    <preference name="orientation" value="portrait" />
    <preference name="fullscreen" value="false" />
    <preference name="InspectableWebview" value="true" />
    <preference name="AndroidEdgeToEdge" value="false" />
    <preference name="android-minSdkVersion" value="23" />
    <preference name="android-targetSdkVersion" value="35" />
    <preference name="AndroidWindowSplashScreenAnimatedIcon" value="www/icons/loading-logo.png" />
    <preference name="AndroidWindowSplashScreenBackground" value="#ffffff" />
    <feature name="NetworkStatus">
        <param name="android-package" value="org.apache.cordova.networkinformation.NetworkManager" />
    </feature>
</widget>
